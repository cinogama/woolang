import woo.std;
import test_tool;

namespace test_gc
{
    var global_list = []:array<dynamic>;

    func main()
    {
        global_list = [1: dynamic,2: dynamic,3: dynamic,4: dynamic,5: dynamic]:array<dynamic>;
        var i = 0;
        var tmp = []:array<array<int>>;
        var arr_a = [[12345678],], arr_b = []:array<array<int>>;
        while (i < 1000_000)
        {
                global_list->add({}: dynamic);
                global_list[-1]["Hello"] = "world": dynamic;
                i+=1;

                tmp->add(arr_a[0]);
                arr_a->remove(0);
                arr_b->add(tmp[0]);
                tmp->remove(0);

                test_assure(arr_b[0][0] == 12345678);

                tmp->add(arr_b[0]);
                arr_b->remove(0);
                arr_a->add(tmp[0]);
                tmp->remove(0);

                test_assure(arr_a[0][0] == 12345678);
        }
        global_list = [];
    }
}

test_function("test_gc.main", test_gc::main);