import woo.std;
import test_tool;

namespace test_static
{
    func static_var_in_func()
    {
        static let mut invoke_count = 0;
        do invoke_count += 1;
        return invoke_count;
    }
    func main()
    {
        do static_var_in_func();
        do static_var_in_func();
        do static_var_in_func();

        test_equal(static_var_in_func(), 4);
    }
}

test_function("test_static.main", test_static::main);