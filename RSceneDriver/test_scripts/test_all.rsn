import rscene.std;
import rscene.coroutine;

var arr = []:array<int>;
var completed = {}:map<int, int>;

func main()
{
	for(var i = 0; i < 10000; i += 1)
	{
		std::coroutine(func(var index:int){
			while(true)
			{
				arr->add(1);
				arr->clear();
				// std::println("wuhu~", index);
				completed->get(index, 0) += 1;
				std::coroutine::yield();
			}
		}, i);
	}
}
std::sleep(1);
std::sleep(1);
std::sleep(1);
main();
while (true)
{
	std::sleep(1);
	std::println(completed);
}


