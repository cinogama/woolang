namespace mini_test
{
using std;

func cost_tm_func()
{
        var beg_tm = time();
        for(var i=0;i<1'0000'0000;i+=1)
                std::co::yield();

        var end_tm = time();
        println("cost:", end_tm - beg_tm);
}

for(var i=0;i<4;i+=1)
        std::co(cost_tm_func);

}

