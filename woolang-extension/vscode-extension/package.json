{
    "name": "woolang-extension",
    "displayName": "woolang extension",
    "description": "woolang is a script programing language with static type system.",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.68.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "woolang",
            "aliases": ["Woolang", "woolang"],
            "extensions": [".wo"],
            "configuration": "./language-configuration.json",
            "icon": {"light": "./image/woolang_logo.png", "dark": "./image/woolang_logo.png"}
        }],
        "grammars": [{
            "language": "woolang",
            "scopeName": "source.woolang",
            "path": "./syntaxes/woolang.tmLanguage.json"
        }]
    }
}